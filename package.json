{
  "name": "@pauloluan/playground",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "packages/*"
  ],
  "scripts": {
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "postinstall": "yarn bootstrap",
    "bootstrap": "lerna bootstrap && yarn build",
    "build": "lerna run --stream build",
    "dev": "lerna run dev --stream --parallel",
    "start": "lerna run start --stream",
    "format": "eslint --fix packages --ext .js,.jsx,.ts,.tsx",
    "test": "lerna run --parallel test",
    "test:watch": "lerna run --parallel test:watch",
    "test:e2e:ci": "lerna run --parallel test:e2e:ci",
    "deps:upgrade": "npx sort-package-json && npx npm-check-updates -u",
    "prestorybook": "rimraf node_modules/.cache/storybook",
    "storybook": "concurrently \"yarn start:storybook\" \"yarn start:shared-ui\"",
    "start:shared-ui": "lerna run --scope @pauloluan/shared-ui dev",
    "start:storybook": "start-storybook -p 6006"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "resolutions": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "core-js": "^3.15.1",
    "framer-motion": "4.1.17",
    "miragejs": "^0.1.41",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-feather": "^2.0.9",
    "react-modal": "^3.14.3",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.3.0",
    "styled-icons": "^10.35.0",
    "styled-media-query": "^2.1.2",
    "web-vitals": "^2.0.1"
  },
  "devDependencies": {
    "@babel/cli": "7.14.5",
    "@babel/core": "^7.14.6",
    "@babel/plugin-transform-runtime": "^7.14.5",
    "@babel/preset-env": "7.14.7",
    "@babel/preset-react": "7.14.5",
    "@babel/preset-typescript": "^7.14.5",
    "@chakra-ui/icons": "^1.0.13",
    "@chakra-ui/react": "^1.6.4",
    "@chakra-ui/theme-tools": "1.1.7",
    "@cypress/react": "^5.9.1",
    "@cypress/vite-dev-server": "^2.0.1",
    "@cypress/webpack-dev-server": "^1.4.0",
